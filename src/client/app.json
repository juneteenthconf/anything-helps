    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": { "babel": { "include": [ "@ui-kitten/components" ] }}
    },
    "extra": {
      "eas": {
        "projectId": "e5c031cf-5a69-429e-a644-b7bf766072dc"
      }
    },
    "owner": "juneteenthconf"
  }
}
